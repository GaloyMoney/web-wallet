{
  "name": "web-wallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:all": "yarn install && yarn run build:node && yarn run build:files && yarn run build:webpack",
    "build:files": "copyfiles -u 1 -e \"src/**/*.ts\" -e \"src/**/*.tsx\" -e \"src/**/*.js\" \"src/**/*\" build/",
    "build:node": "rm -rf build && tsc --sourceMap false --skipLibCheck true",
    "build:webpack": "cross-env-file webpack --mode=production --env=prod",
    "code:check": "yarn tsc:check && yarn eslint:check && yarn prettier:check",
    "dev:bundler": "cross-env-file webpack -w --mode=development --env=dev",
    "dev:server": "cross-env-file tsnd --files --respawn src/server/server.ts --ignore-watch node_modules,public",
    "eslint:check": "eslint . --ext ts --ext tsx",
    "eslint:fix": "eslint . --ext ts --ext tsx --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prod:logs": "pm2 logs --update-env web-walletProd",
    "prod:reload": "pm2 reload --update-env web-walletProd && yarn run prod:logs",
    "prod:start": "cross-env-file pm2 start -i max build/server/server.js --exp-backoff-restart-delay=150 --update-env --name web-walletProd",
    "prod:stop": "pm2 stop web-walletProd",
    "test:verify": "jest --coverage",
    "test": "cross-env-file -p .env.local jest",
    "tsc:check": "tsc --pretty --noEmit --skipLibCheck",
    "dev:codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "autoprefixer": "^10.4.0",
    "body-parser": "^1.19.1",
    "cookie-session": "^2.0.0",
    "copyfiles": "^2.4.1",
    "cross-env-file": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "css-loader": "^6.5.1",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "graphql": "^16.2.0",
    "helmet": "^4.6.0",
    "history": "^5.2.0",
    "i18n-js": "^3.8.0",
    "intl-tel-input": "^17.0.15",
    "mini-css-extract-plugin": "^2.4.5",
    "morgan": "^1.10.0",
    "normalize.css": "^8.0.1",
    "pm2": "^5.1.2",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "react": "^18.0.0-rc.0-next-20212349a-20211223",
    "react-dom": "^18.0.0-rc.0-next-20212349a-20211223",
    "react-error-boundary": "^3.1.4",
    "regenerator-runtime": "^0.13.9",
    "serialize-javascript": "^6.0.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-session": "^2.0.43",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/i18n-js": "^3.8.2",
    "@types/intl-tel-input": "^17.0.4",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/serialize-javascript": "^5.0.1",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typesync": "^0.8.0"
  }
}
